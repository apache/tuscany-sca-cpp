  <service name="subtract">
   <documentation>-</documentation>
  </service>
  <reference name="value1"/>
  <reference name="value2"/>
 </component>
 <component x="90" y="80" name="sum" title="sum" color="magenta1">
  <implementation.python script="nuvem/sum_.py"/>
  <service name="sum"/>
  <reference name="l"/>
 </component>
</composite>
